meta {
  name: get file by id
  type: http
  seq: 3
}

get {
  url: {{baseUrl}}/files/{{fileId}}
  body: none
  auth: inherit
}

headers {
  authorization: Bearer {{token}}
}

tests {
  test("should return file by ID", function() {
    expect(res.getStatus()).to.equal(200);
    expect(res.getBody()).to.have.property('data');
    expect(res.getBody().data).to.have.property('id', bru.getEnvVar("fileId"));
    expect(res.getBody()).to.have.property('status', 'success');
  });
}
